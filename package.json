{
  "name": "@lowlighter/rakun",
  "version": "1.0.4",
  "description": "A parser for anime torrent to extract features",
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "dependencies": {},
  "devDependencies": {
    "axios": "^0.19.2",
    "colors": "^1.4.0",
    "eslint": "^7.0.0",
    "jest": "^26.0.1",
    "minimist": "^1.2.5",
    "npm-run-all": "^4.1.5",
    "simple-git": "^2.5.0",
    "typescript": "^4.0.0-dev.20200520"
  },
  "scripts": {
    "true": "exit 0",
    "clean": "npx tsc --build --clean",
    "build": "npx tsc",
